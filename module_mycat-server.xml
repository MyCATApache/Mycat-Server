  <patternset id="excluded.from.module.mycat-server">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.mycat-server">
    <patternset refid="excluded.from.module.mycat-server"/>
  </patternset>
  
  <path id="mycat-server.module.sourcepath">
    <dirset dir="${module.mycat-server.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="mycat-server.module.test.sourcepath">
    <dirset dir="${module.mycat-server.basedir}">
      <include name="src/test/java"/>
      <include name="src/test/resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.mycat-server" depends="compile.module.mycat-server.production,compile.module.mycat-server.tests" description="Compile module Mycat-server"/>
  
  <target name="compile.module.mycat-server.production" depends="register.custom.compilers" description="Compile module Mycat-server; production classes">
    <mkdir dir="${mycat-server.output.dir}"/>
    <javac2 destdir="${mycat-server.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.mycat-server}/javac">
      <compilerarg line="${compiler.args.mycat-server}"/>
      <bootclasspath refid="mycat-server.module.bootclasspath"/>
      <classpath refid="mycat-server.module.production.classpath"/>
      <src refid="mycat-server.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.mycat-server"/>
    </javac2>
    
    <copy todir="${mycat-server.output.dir}">
      <fileset dir="${module.mycat-server.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.mycat-server.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.mycat-server.tests" depends="register.custom.compilers,compile.module.mycat-server.production" description="compile module Mycat-server; test classes" unless="skip.tests">
    <mkdir dir="${mycat-server.testoutput.dir}"/>
    <javac2 destdir="${mycat-server.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.mycat-server}/javac">
      <compilerarg line="${compiler.args.mycat-server}"/>
      <bootclasspath refid="mycat-server.module.bootclasspath"/>
      <classpath refid="mycat-server.module.classpath"/>
      <src refid="mycat-server.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.mycat-server"/>
    </javac2>
    
    <copy todir="${mycat-server.testoutput.dir}">
      <fileset dir="${module.mycat-server.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.mycat-server.basedir}/src/test/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.mycat-server" description="cleanup module">
    <delete dir="${mycat-server.output.dir}"/>
    <delete dir="${mycat-server.testoutput.dir}"/>
  </target>
